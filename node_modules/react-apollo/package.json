    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.1",
    "@apollo/react-components": "^3.1.1",
    "@apollo/react-hoc": "^3.1.1",
    "@apollo/react-hooks": "^3.1.1",
    "@apollo/react-ssr": "^3.1.1"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
